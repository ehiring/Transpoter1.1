                            <label>First Name</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                <span class="mandatory" style="margin-left: 35px;">*</span>
                                <input name="Fname" type="text" id="ContentPlaceHolder1_txtFname" class="form-control capital" placeholder="Enter First Name">
